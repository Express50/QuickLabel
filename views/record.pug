extends variables

block records
    div(id="two" class="searchable" style={"position": "relative", "z-index": "-2", left: "40%", width: "60%"})
        if neighbourhood_data !== undefined
            if neighbourhood_data.length > 0
                h2= "Neighbourhood"
                p
                    each element in neighbourhood_data
                        each line in element.split(/\\n/)
                            != line
                            br
                        br
        h2= "Charts"
        each element in text_data
            each line in element.split(/\\n/)
                    = line
                    br
            br